{
  "name": "quicint",
  "version": "0.3.0",
  "description": "Quick initialize HTML5 EJS Boilerplate",
  "main": "src/assets/js/main.js",
  "scripts": {
    "dev": "NODE_ENV=development gulp",
    "production": "NODE_ENV=production gulp publish",
    "json-check": "NODE_ENV=development gulp json_check",
    "img": "NODE_ENV=development gulp img_reset",
    "lint:stylesheet": "stylelint --fix src/assets/css/scss/**/*.scss",
    "format:stylesheet": "prettier-stylelint --quiet --write src/assets/css/scss/**/*.scss",
    "lint:js": "eslint src/assets/js/main.js",
    "format:js": "eslint --fix src/assets/js/**/*.js",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "yarn format:stylesheet && yarn lint:stylesheet",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TsubasaHiga/Quicint.git"
  },
  "keywords": [
    "HTML5 EJS Boilerplate",
    "HTML TEMPLATE",
    "EJS"
  ],
  "author": "HigaTsubasa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TsubasaHiga/Quicint/issues"
  },
  "homepage": "https://github.com/TsubasaHiga/Quicint#readme",
  "browserslist": [
    "> 0.5% in JP"
  ],
  "prettier": {},
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.7",
    "colors": "^1.4.0",
    "connect-ssi": "^1.1.1",
    "crypto": "^1.0.1",
    "css-declaration-sorter": "^4.0.1",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "date-utils": "^1.2.21",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs": "0.0.1-security",
